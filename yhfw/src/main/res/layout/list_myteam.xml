<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
   <ImageView
       android:id="@+id/list_my_team_img"
       android:layout_marginRight="@dimen/dp_10"
       android:layout_marginTop="@dimen/dp_30"
       android:layout_marginBottom="@dimen/dp_30"
       android:layout_marginLeft="@dimen/dp_20"
       android:src="@mipmap/icon_road"
       android:layout_width="@dimen/dp_60"
       android:layout_height="@dimen/dp_60" />
    <TextView
        android:id="@+id/list_my_team_tv_name"
        android:layout_alignTop="@+id/list_my_team_img"
        android:layout_toRightOf="@+id/list_my_team_img"
        android:text="姓名："
        android:textSize="@dimen/sp_15"
        android:textColor="@color/color_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/list_my_team_inset_dt"
        android:layout_toRightOf="@+id/list_my_team_img"
        android:layout_centerVertical="true"
        android:text="注册时间："
        android:textSize="@dimen/sp_15"
        android:textColor="@color/color_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/list_my_team_tv_tg"
        android:layout_alignBottom="@+id/list_my_team_img"
        android:layout_toRightOf="@+id/list_my_team_img"
        android:text="推广"
        android:textSize="@dimen/sp_15"
        android:textColor="@color/color_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/list_my_team_sy"
        android:layout_marginTop="@dimen/dp_40"
        android:textSize="@dimen/sp_15"
        android:textColor="@color/color_black"
        android:layout_marginRight="@dimen/dp_20"
        android:text="收益"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/list_my_team_tv_sy"
        android:layout_below="@+id/list_my_team_sy"
        android:layout_marginTop="@dimen/dp_5"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/red"
        android:layout_marginRight="@dimen/dp_20"
        android:text="0.00元"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>
